.header-wrapper {
	width: 100%;
	height: 65px;
	padding-bottom: 5px;
	background: pink;
	display: flex;
	align-items: center;
  font-size: 16px;
  position: fixed;
  top: 0;
  z-index: 999;
}

.image {
  width: 25px;
  height: 25px;
  margin-left: 15rpx;
  margin-right: 20rpx;
}

.position {
  margin-top: 70rpx;
}

.tabWrapper {
  width: 100%;
  height: 35px;
  background-color: white;
  margin-top: -35rpx;
  display: flex;
  font-size: 12px;
  color: grey;
}

.item {
  flex: 1;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tab-active {
  color: #d43c33;
  border-bottom: 1px solid #d43c33;
}

page {
  background-color: #F5F5F5
}